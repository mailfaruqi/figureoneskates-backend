generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(cuid()) // abc123xyz
  slug        String // jackson-freestyle
  name        String // Jackson Freestyle
  imageURL    String // https://example.com/jackson-freestyle-1690212627.jpg
  description String // This skate is ideal for skaters landing single jumps and starting doubles
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
